{
    "compilerOptions": {
        "module": "nodenext",
        "target": "es5",
        "moduleResolution": "nodenext",
        "baseUrl": ".",
        "paths": {
            "@/*": [ "./resources/ts/*" ],
        }
    },
    "include": [
        "./resources/ts/**/*.ts",
        "./resources/ts/**/*.tsx",
        "./resources/ts/**/*.js",
    ],
}
